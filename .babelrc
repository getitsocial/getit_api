{  
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathPrefix": "~",
            "rootPathSuffix": "src"    
          },
          {
            "rootPathPrefix": "!",
            "rootPathSuffix": "src/models"    
          }
        ]
      }
    ],
    "@babel/plugin-transform-parameters",
    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-proposal-optional-chaining"
  ],
  "sourceMaps": "both",
  "highlightCode": true
}
